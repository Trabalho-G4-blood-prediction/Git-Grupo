[access]
	inheritFrom = Private-Projects
[submit]
[access "refs/*"]
	owner = group user/FelipeN494
[access "refs/for/refs/*"]
	label-Code-Review = -2..+2 group user/FelipeN494
	label-Verified = -1..+1 group user/FelipeN494
	push = group user/FelipeN494
	read = group user/FelipeN494
	rebase = group user/FelipeN494
	removeReviewer = group user/FelipeN494
	submit = group user/FelipeN494
[access "refs/heads/*"]
	create = group user/FelipeN494
	push = +force group user/FelipeN494
	pushMerge = group user/FelipeN494
	read = group user/FelipeN494
[access "refs/tags/*"]
	push = group user/FelipeN494
	push = +force group user/FelipeN494
